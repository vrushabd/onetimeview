{
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/view/([^/]+)",
      "dest": "/view.html"
    },
    {
      "src": "/create",
      "dest": "/create.html"
    },
    {
      "src": "/privacy",
      "dest": "/privacy.html"
    },
    {
      "src": "/terms",
      "dest": "/terms.html"
    },
    {
      "src": "/expired",
      "dest": "/expired.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}