    {
      "source": "/create",
      "destination": "/frontend/create.html"
    },
    {
      "source": "/view/(.*)",
      "destination": "/frontend/view.html"
    },
    {
      "source": "/privacy",
      "destination": "/frontend/privacy.html"
    },
    {
      "source": "/terms",
      "destination": "/frontend/terms.html"
    },
    {
      "source": "/expired",
      "destination": "/frontend/expired.html"
    }
  ]
}
