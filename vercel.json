{
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/frontend/static/$1"
    },
    {
      "src": "/view/([^/]+)",
      "dest": "/frontend/view.html"
    },
    {
      "src": "/create",
      "dest": "/frontend/create.html"
    },
    {
      "src": "/privacy",
      "dest": "/frontend/privacy.html"
    },
    {
      "src": "/terms",
      "dest": "/frontend/terms.html"
    },
    {
      "src": "/expired",
      "dest": "/frontend/expired.html"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html"
    }
  ]
}